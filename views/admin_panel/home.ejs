<%- include('_navbar.ejs') %>
    <div class="home">
        <h1>This is Admin Panel</h1>
    </div>
    <div class="login-containers">
        <h1>Login</h1>
        <form action="/login" method="post">
            <!-- Add your login form fields here -->
            <div class="form-group1">
                <label for="username" class="text1">Username:</label>
                <input type="text" id="username" class="log" name="username" required>
            </div>
            <div class="form-group1">
                <label for="password" class="text1">Password:</label>
                <input type="password" id="password" class="log" name="password" required>
            </div>
            <button class="button" type="submit">Login</button>
        </form>
    </div>

    <script src="/script/home.js"></script>